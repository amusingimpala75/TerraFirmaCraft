{
  "schemaVersion": 1,
  "id": "tfc",
  "version": "${version}",

  "name": "TerraFirmaCraft",
  "description": "TerraFirmaCraft - The Next Generation By: AlcatrazEscapee, Disastermoo, Dries007, Bunsan, Claycorp, DavidTriphon, EERussianGuy, Gaelmare, and many others. Based on the original work by Bioxx, Dunk, and Kitty. Port to Fabric by AmusingImpala75",
  "authors": [
    "AlcatrazEscapee"
  ],
  "contact": {
    "sources": "https://github.com/amusingimpala75/TerraFirmaCraft"
  },

  "license": "EUPL-1.2",
  "icon": "pack.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.dries007.tfc.TerraFirmaCraft"
    ],
    "client": [
      "net.dries007.tfc.client.TerraFirmaCraftClient"
    ],
    "cardinal-components": [
      "net.dries007.tfc.fabric.cca.Components"
    ],
    "modmenu": [
      "net.dries007.tfc.config.ModMenuIntegration"
    ]
  },
  "accessWidener" : "tfc.accesswidener",
  "mixins": [
    "tfc.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },

  "custom": {
    "cardinal-components": [
      "tfc:forge",
      "tfc:heat",
      "tfc:world_tracking",
      "tfc:chunk_data"
    ]
  }
}
